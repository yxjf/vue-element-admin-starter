{
  "name": "vue-element-admin-starter",
  "version": "1.0.1",
  "description": "A vue admin template with ElementUI for most management system",
  "author": "jnoodle <fullstack@126.com>",
  "scripts": {
    "serve": "vue-cli-service serve --open  --mode development",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build:test": "vue-cli-service build --mode test",
    "lint-staged": "lint-staged",
    "lint:style": "stylelint  --fix \"src/**/*.less\" --syntax less",
    "prettier": "node ./scripts/prettier.js"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "axios-mock-adapter": "^1.17.0",
    "brace": "^0.11.1",
    "core-js": "^3.4.0",
    "element-ui": "^2.12.0",
    "font-awesome": "4.7",
    "lodash": "^4.17.19",
    "mockjs": "^1.1.0",
    "moment": "^2.29.4",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "store": "^2.0.12",
    "tiptap": "^1.26.4",
    "tiptap-extensions": "^1.28.4",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.5",
    "@vue/cli-plugin-eslint": "^4.0.5",
    "@vue/cli-service": "^4.0.5",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.6.0",
    "eslint-plugin-vue": "^6.0.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.4.2",
    "prettier": "^1.19.1",
    "stylelint": "^11.1.1",
    "stylelint-config-prettier": "^6.0.0",
    "stylelint-config-standard": "^19.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "engines": {
    "node": ">= 8.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "post-commit": "git update-index --again"
  },
  "license": "MIT",
  "lint-staged": {
    "**/*.{js,vue,less}": [
      "node ./scripts/prettier.js",
      "git add"
    ],
    "src/**/*.{js,vue}": "vue-cli-service lint",
    "src/**/*.less": "stylelint  --fix \"src/**/*.less\" --syntax less"
  }
}
